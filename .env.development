# 开发环境配置
# 应用标题
VITE_APP_TITLE=Vue Strict (开发环境)

# API基础URL
VITE_API_BASE_URL=http://localhost:3000/api

# 是否启用Mock数据
VITE_USE_MOCK=true

# 开发服务器端口
VITE_PORT=5173

# 是否自动打开浏览器
VITE_OPEN=true

# 是否启用热更新
VITE_HMR=true

# 开发环境日志级别
VITE_LOG_LEVEL=info

# 是否显示开发工具
VITE_SHOW_DEVTOOLS=true

# 是否启用源码映射
VITE_SOURCEMAP=true

# 是否删除console（开发环境保留）
VITE_DROP_CONSOLE=false